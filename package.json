{
  "name": "@stellaraf/jest",
  "version": "2.0.2",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Matt Love <matt@stellar.tech>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist/",
    "esm/"
  ],
  "eslintConfig": {
    "root": true,
    "extends": [
      "@stellaraf"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "semi": true
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@stellaraf/eslint-config": "^1.1.6",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "esbuild": "^0.17.18",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint --quiet src/**/*",
    "build:js": "ts-node-esm build.ts",
    "build:ts": "tsc",
    "build": "pnpm run build:js && pnpm run build:ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "peerDependencies": {
    "esbuild": "^0.17.18",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
